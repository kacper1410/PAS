<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE payara-web-app PUBLIC "-//Payara.fish//DTD Payara Server 4 Servlet 3.0//EN"
        "https://raw.githubusercontent.com/payara/Payara-Enterprise-Documentation/master/docs/modules/ROOT/pages/schemas/payara-web-app_4.dtd">
<payara-web-app>
    <context-root>/PAS</context-root>
    <security-role-mapping>
        <role-name>Administrator</role-name>
        <group-name>Admins</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Employee</role-name>
        <group-name>Employees</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Client</role-name>
        <group-name>Clients</group-name>
    </security-role-mapping>
    <class-loader/>
</payara-web-app>
