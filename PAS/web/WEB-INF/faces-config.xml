<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
  <!-- TODO LINKs -->
  <navigation-rule>
    <from-view-id/>
    <navigation-case>
      <from-outcome>userList</from-outcome>
      <to-view-id>/users.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>resourceList</from-outcome>
      <to-view-id>/resources.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>borrows</from-outcome>
      <to-view-id>/borrows.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>main</from-outcome>
      <to-view-id>/index.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
</faces-config>
